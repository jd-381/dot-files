[alias]
	# Confirms name and email of last commit
	author = log -1 --format='Author: %an <%ae>'
	
	# Worktree shortcuts
	wt = worktree
	wta = worktree add
	wtl = worktree list
	wtr = worktree remove
	wtp = worktree prune
	wtmv = worktree move
[fetch]
	prune = true
[include]
	# Machine specific stuff
	path = ~/.gitconfig.local
[user]
	email = jd-381@users.noreply.github.com

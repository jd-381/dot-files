[alias]
	all = add .
	author = log -1 --format='Author: %an <%ae>' # Prints name and email of last commit
	br = branch
	ci = commit
	cim = commit -m 
	co = checkout   
	re = restore
	res = restore --staged
    st = status
	wt = worktree
	wta = worktree add
	wtl = worktree list
	wtr = worktree remove
	wtp = worktree prune
	wtmv = worktree move
[fetch]
	prune = true
[include]
	path = ~/.gitconfig.local # Machine specific stuff
